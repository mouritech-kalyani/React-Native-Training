{
  "name": "MyAwesomeProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/react-native": "^8.0.0",
    "@types/react-native": "^0.65.0",
    "@types/react-test-renderer": "^17.0.1",
    "axios": "^0.24.0",
    "react": "17.0.2",
    "react-native": "0.66.0",
    "react-native-flipper": "^0.117.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-vector-icons": "^8.1.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-flipper": "^2.0.1",
    "redux-saga": "^1.1.3",
    "sessionstorage": "^0.1.0",
    "ts-jest": "^27.0.7",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.16.0",
    "@babel/runtime": "^7.15.4",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.29",
    "@types/react-native": "^0.65.5",
    "@types/react-native-vector-icons": "^6.4.9",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^27.3.1",
    "eslint": "^8.0.1",
    "jest": "^27.3.1",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "coveragePathIgnorePatterns": [
      "node_modules",
      ".stories.ts",
      ".saga.ts",
      "<rootDir>/src/app.tsx",
      "<rootDir>/src/api",
      "<rootDir>/src/local",
      "<rootDir>/src/types",
      "<rootDir>/src/navigation",
      "<rootDir>/src/constants",
      "<rootDir>/src/context",
      "<rootDir>/src/assets",
      "<rootDir>/src/components/index.ts",
      "<rootDir>/src/components/AppHeader/stories",
      "<rootDir>/src/components/AppLoader/stories",
      "<rootDir>/src/components/Dialog/stories",
      "<rootDir>/src/components/SVGIcon/stories",
      "<rootDir>/src/components/Button/stories",
      "<rootDir>/src/routes/RequesForGuestPass",
      "<rootDir>/src/routes/Support",
      "<rootDir>/src/utils",
      "<rootDir>/src/styles/ThemeProvider.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|react-native|react-native-vector-icons|react-navigation|react-(native|universal|navigation)-(.*)|@react-native-community/(.*)|@react-navigation/(.*)|bs-platform|(@[a-zA-Z]+/)?(bs|reason|rescript)-(.*)+|@react-navigation/.*)"
    ],
    "testEnvironment": "node"
  }
}
